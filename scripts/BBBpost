if [ ! -d "scripts" ]; then
  cd ..
fi

cd scripts

python vel_v_time.py
python pos_v_time.py
python multi_v_time.py

cd ..

OUTDIR="$1"
if [ "$OUTDIR" == "" ]; then
  echo No output folder specified. Setting to out.
  OUTDIR="out"
fi

if [ ! -d "$OUTDIR" ]; then
  echo Folder does not exist. making it now.
  mkdir $OUTDIR
fi

mv build/*.csv build/*.jpg $OUTDIR/
mv scripts/*.png $OUTDIR/

echo Complete.
